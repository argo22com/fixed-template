        "heading_Nmdam7": {
          "type": "heading",
          "settings": {
            "heading": "<h3><strong>RECYKLOVANÉ POUZDRO RESTORY<\/strong><\/h3>",
            "heading_size": "h2"
          }
        },
        "text_eqbpAU": {
          "type": "text",
          "settings": {
            "text": "<p>Stejné pouzdro, stejná cena, stejná kvalita.<br\/>Vyrobeno z recyklovaného materiálu z nepoužitých TPU pouzder v EU.<\/p>"
          }
        }
      },
